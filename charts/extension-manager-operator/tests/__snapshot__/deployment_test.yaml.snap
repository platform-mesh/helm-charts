deployment with external kubeconfig:
  1: |
    apiVersion: apps/v1
    kind: Deployment
    metadata:
      name: extension-manager-operator-operator
      namespace: NAMESPACE
    spec:
      revisionHistoryLimit: 3
      selector:
        matchLabels:
          app: extension-manager-operator
      template:
        metadata:
          annotations:
            sidecar.istio.io/inject: "false"
          labels:
            app: extension-manager-operator
            control-plane: controller-manager
        spec:
          automountServiceAccountToken: true
          containers:
            - args:
                - operator
                - --kcp-enabled
                - --leader-elect
                - --metrics-bind-address=:9090
                - --health-probe-bind-address=:8090
                - --log-level=warn
                - --region=local
                - --environment=local
                - --image-tag=1.0.0
                - --image-name="ghcr.io/platform-mesh/extension-manager-operator"
                - --shutdown-timeout=1m
                - --max-concurrent-reconciles=10
              env: null
              image: ghcr.io/platform-mesh/extension-manager-operator:1.0.0
              imagePullPolicy: IfNotPresent
              livenessProbe:
                failureThreshold: 1
                httpGet:
                  path: /healthz
                  port: 8090
                periodSeconds: 10
              name: extension-manager-operator
              ports:
                - containerPort: 9090
                  name: metrics
                  protocol: TCP
                - containerPort: 8090
                  name: health-port
                  protocol: TCP
              readinessProbe:
                httpGet:
                  path: /readyz
                  port: 8090
                initialDelaySeconds: 5
                periodSeconds: 10
              resources:
                limits:
                  memory: 512Mi
                requests:
                  cpu: 40m
                  memory: 50Mi
              securityContext:
                readOnlyRootFilesystem: true
                runAsNonRoot: true
                seccompProfile:
                  type: RuntimeDefault
              startupProbe:
                failureThreshold: 30
                httpGet:
                  path: /readyz
                  port: 8090
                periodSeconds: 10
              volumeMounts:
                - mountPath: /test
                  name: test
          securityContext:
            runAsNonRoot: true
            seccompProfile:
              type: RuntimeDefault
          serviceAccountName: extension-manager-operator
          terminationGracePeriodSeconds: 10
          volumes:
            - name: test
              secret:
                secretName: test

match the default snapshot:
  1: |
    apiVersion: rbac.authorization.k8s.io/v1
    kind: ClusterRole
    metadata:
      name: terminal-controller-manager
    rules:
      - apiGroups:
          - ""
        resources:
          - pods
        verbs:
          - create
          - delete
          - get
          - list
          - watch
      - apiGroups:
          - ""
        resources:
          - services
        verbs:
          - create
          - delete
          - get
          - list
          - patch
          - update
          - watch
      - apiGroups:
          - gateway.networking.k8s.io
        resources:
          - httproutes
        verbs:
          - create
          - delete
          - get
          - list
          - patch
          - update
          - watch
      - apiGroups:
          - ""
        resources:
          - namespaces
          - events
        verbs:
          - get
          - list
          - watch
          - create
          - update
          - patch
      - apiGroups:
          - coordination.k8s.io
        resources:
          - leases
        verbs:
          - create
          - delete
          - get
          - list
          - patch
          - update
          - watch
  2: |
    apiVersion: rbac.authorization.k8s.io/v1
    kind: ClusterRoleBinding
    metadata:
      name: terminal-controller-manager
    roleRef:
      apiGroup: rbac.authorization.k8s.io
      kind: ClusterRole
      name: terminal-controller-manager
    subjects:
      - kind: ServiceAccount
        name: terminal-controller-manager
        namespace: platform-mesh-system
  3: |
    apiVersion: apps/v1
    kind: Deployment
    metadata:
      name: terminal-controller-manager
    spec:
      replicas: 1
      revisionHistoryLimit: 3
      selector:
        matchLabels:
          app: terminal-controller-manager
      template:
        metadata:
          annotations: null
          labels:
            app: terminal-controller-manager
            control-plane: controller-manager
        spec:
          automountServiceAccountToken: true
          containers:
            - args:
                - operator
                - --leader-elect
                - --metrics-bind-address=:9090
                - --health-probe-bind-address=:8090
                - --log-level=warn
                - --region=local
                - --environment=local
                - --image-tag=0.0.0
                - --image-name="ghcr.io/platform-mesh/terminal-controller-manager"
                - --shutdown-timeout=1m
                - --max-concurrent-reconciles=10
              env:
                - name: KCP_API_EXPORT_ENDPOINT_SLICE_NAME
                  value: terminal.platform-mesh.io
                - name: TERMINAL_IMAGE
                  value: ghcr.io/platform-mesh/terminal:latest
                - name: TERMINAL_NAMESPACE
                  value: terminal-sessions
                - name: TERMINAL_LIFETIME
                  value: 2h
                - name: GATEWAY_NAME
                  value: k8sapi-gateway
                - name: GATEWAY_NAMESPACE
                  value: platform-mesh-system
                - name: SUBROUTINES_LIFETIME_ENABLED
                  value: "true"
                - name: SUBROUTINES_POD_ENABLED
                  value: "true"
                - name: SUBROUTINES_SERVICE_ENABLED
                  value: "true"
                - name: SUBROUTINES_HTTPROUTE_ENABLED
                  value: "true"
              image: ghcr.io/platform-mesh/terminal-controller-manager:0.0.0
              imagePullPolicy: IfNotPresent
              livenessProbe:
                failureThreshold: 1
                httpGet:
                  path: /healthz
                  port: 8090
                periodSeconds: 10
              name: terminal-controller-manager
              ports:
                - containerPort: 9090
                  name: metrics
                  protocol: TCP
                - containerPort: 8090
                  name: health-port
                  protocol: TCP
              readinessProbe:
                httpGet:
                  path: /readyz
                  port: 8090
                initialDelaySeconds: 5
                periodSeconds: 10
              resources:
                limits:
                  memory: 512Mi
                requests:
                  cpu: 40m
                  memory: 50Mi
              securityContext:
                readOnlyRootFilesystem: true
                runAsNonRoot: true
                seccompProfile:
                  type: RuntimeDefault
              startupProbe:
                failureThreshold: 30
                httpGet:
                  path: /readyz
                  port: 8090
                periodSeconds: 10
              volumeMounts: null
          securityContext:
            runAsNonRoot: true
            seccompProfile:
              type: RuntimeDefault
          serviceAccountName: terminal-controller-manager
          terminationGracePeriodSeconds: 10
          volumes: null
  4: |
    apiVersion: v1
    kind: Namespace
    metadata:
      labels:
        app.kubernetes.io/managed-by: terminal-controller-manager
      name: terminal-sessions
  5: |
    apiVersion: v1
    kind: ServiceAccount
    metadata:
      name: terminal-controller-manager

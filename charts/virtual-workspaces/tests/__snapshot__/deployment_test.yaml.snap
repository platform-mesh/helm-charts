                  name: authentication
                - mountPath: /client-ca
                  name: client-ca-secret
                - mountPath: /certs
                  name: vws-cert
                - mountPath: /requestheader-client-ca
                  name: requestheader-client-ca
          imagePullSecrets:
            - name: github
          securityContext:
            runAsNonRoot: true
            seccompProfile:
              type: RuntimeDefault
          serviceAccountName: virtual-workspaces
          terminationGracePeriodSeconds: 10
          volumes:
            - name: external-api-server
              secret:
                secretName: account-operator-kubeconfig
            - name: authentication
              secret:
                secretName: portal-kubeconfig
            - name: client-ca-secret
              secret:
                secretName: kcp-ca
            - name: vws-cert
              secret:
                secretName: kcp-virtual-workspaces-cert
            - name: requestheader-client-ca
              secret:
                secretName: kcp-requestheader-client-ca
  2: |
    apiVersion: autoscaling/v2
    kind: HorizontalPodAutoscaler
    metadata:
      name: virtual-workspaces
      namespace: NAMESPACE
    spec:
      maxReplicas: 10
      metrics:
        - resource:
            name: cpu
            target:
              averageUtilization: 80
              type: Utilization
          type: Resource
      minReplicas: 1
      scaleTargetRef:
        apiVersion: apps/v1
        kind: Deployment
        name: virtual-workspaces
  3: |
    apiVersion: v1
    imagePullSecrets:
      - name: github
    kind: ServiceAccount
    metadata:
      name: virtual-workspaces
  4: |
    apiVersion: v1
    kind: Service
    metadata:
      name: virtual-workspaces
    spec:
      ports:
        - port: 8443
          protocol: TCP
          targetPort: 8443
      selector:
        app: virtual-workspaces

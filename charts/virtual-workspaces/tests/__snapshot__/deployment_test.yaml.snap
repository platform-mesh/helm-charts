virtual-workspaces match the snapshot:
  1: |
    apiVersion: apps/v1
    kind: Deployment
    metadata:
      name: virtual-workspaces
    spec:
      replicas: null
      revisionHistoryLimit: 3
      selector:
        matchLabels:
          app: virtual-workspaces
      template:
        metadata:
          annotations: null
          labels:
            app: virtual-workspaces
        spec:
          automountServiceAccountToken: true
          containers:
            - args:
                - start
                - --tls-cert-file=/certs/tls.crt

                - --authentication-skip-lookup
                - --client-ca-file=/client-ca/tls.crt
                - --requestheader-client-ca-file=/requestheader-client-ca/tls.crt
                - --requestheader-username-headers=X-Remote-User
                - --requestheader-group-headers=X-Remote-Group
                - --requestheader-extra-headers-prefix=X-Remote-Extra-
                - --secure-port=8443
                - --server-url=https://kcp-front-proxy.openmfp-system:8443
              image: ghcr.io/platform-mesh/virtual-workspaces:0.0.0
              imagePullPolicy: IfNotPresent
              name: virtual-workspaces
              ports:
                - containerPort: 8443
                  name: vws
                  protocol: TCP
              resources:
                limits:
                  memory: 512Mi
                requests:
                  cpu: 40m
                  memory: 50Mi
              securityContext:
                readOnlyRootFilesystem: true
                runAsNonRoot: true
                seccompProfile:
                  type: RuntimeDefault
              volumeMounts:
